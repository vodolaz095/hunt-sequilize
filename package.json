  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vodolaz095/hunt-sequilize/issues"
  },
  "dependencies": {
    "mysql": ">=2.3.2",
    "pg": ">=3.3.0",
    "sqlite3": ">=2.2.3",
    "colors": ">=0.6.2",
    "sequelize": ">=1.7.9"
  },
  "devDependencies": {
    "async":">=0.9.0",
    "hunt":">=0.2.0",
    "mocha": ">=1.20.0",
    "istanbul": "*",
    "should": "3.x",
    "jsdoc": "*",
    "sqlite3": "*"
