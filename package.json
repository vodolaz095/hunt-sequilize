{
  "name": "hunt-sequilize",
  "version": "0.0.1",
  "description": "Use Sequilize.js models in Hunt.js application",
  "main": "index.js",
  "engines": {
    "node": ">=0.10.29",
    "npm": ">=1.4.3"
  },
  "os": [ "linux", "darwin" ],
  "repository": {
    "type": "git",
    "url": "git://github.com/vodolaz095/hunt-sequilize.git"
